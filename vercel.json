{
  "rewrites": [
    {
      "source": "/ton-year(.*)",
      "destination": "'https://ton-year.readthedocs.io/$1"
    },
    {
      "source": "/cmip6-downscaling(.*)",
      "destination": "https://cmip6-downscaling.readthedocs.io/$1"
    },
    {
      "source": "/ndpyramid(.*)",
      "destination": "https://ndpyramid.readthedocs.io/$1"
    },
    {
      "source": "/charts(.*)",
      "destination": "https://charts.docs.carbonplan.org/charts$1"
    },
    {
      "source": "/maps(.*)",
      "destination": "https://maps.docs.carbonplan.org/maps$1"
    }
  ],
  "github": {
    "silent": true
  }
}
